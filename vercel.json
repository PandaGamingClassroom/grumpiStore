{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/grumpi-store"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://grumpi-store-server.es/"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
